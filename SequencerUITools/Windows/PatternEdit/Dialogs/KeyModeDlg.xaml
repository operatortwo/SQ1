<Window x:Class="KeyModeDlg"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SequencerUITools"
        mc:Ignorable="d"
        Title="Key Mode" Height="300" Width="450" WindowStartupLocation="CenterOwner" Loaded="Window_Loaded" Closing="Window_Closing" WindowStyle="ToolWindow">
    <Grid>
        <Button x:Name="btnOk" Content="OK" HorizontalAlignment="Left" Margin="141,0,0,20" VerticalAlignment="Bottom" Width="50" IsDefault="True"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="225,0,0,20" VerticalAlignment="Bottom" IsCancel="True" Width="50"/>
        <ComboBox x:Name="cmbKeyMode" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="137"/>
        <TextBox x:Name="tbPatInfo" Margin="166,10,10,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="84" IsReadOnly="True" Background="WhiteSmoke"/>

    </Grid>
</Window>
